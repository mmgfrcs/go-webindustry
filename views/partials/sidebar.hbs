<!-- Partial -->
<aside class="fixed w-64 h-screen top-0 z-10" aria-label="Sidebar">
  <div class="h-full overflow-y-auto py-4 px-3 bg-gray-100 dark:bg-gray-800">
    <a href="https://flowbite.com/" class="flex items-center pl-2.5 mb-5">
      <img src="https://img.icons8.com/clouds/100/000000/bbb.png" class="mr-3 h-6 sm:h-9" alt="Flowbite Logo" />
      <span class="self-center text-xl font-semibold whitespace-nowrap dark:text-white">Web Industry</span>
    </a>
    <ul class="space-y-2">
      {{#each SidebarItems}}
        {{#ifeq type "item"}}
        <li>
          {{>partials/utilities/sidebar-item text=Text}}
        </li>
        {{/ifeq}}
        {{#ifeq type "item-label"}}
        <li>
          {{>partials/utilities/sidebar-item-label text=Text indicator=Label}}
        
        </li>
        {{/ifeq}}
        {{#ifeq type "dropdown"}}
        <li>
          {{>partials/utilities/sidebar-dropdown text="Entities" items=Items}}
        </li>
        {{/ifeq}}
      {{/each}}
    </ul>
  </div>
</aside>